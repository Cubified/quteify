{
  "manifest_version": 2,
  "name": "quteify",
  "description": "Adds qutebrowser-like features to Chrome",
  "version": "0.1.0",
  "homepage_url": "https://github.com/Cubified/quteify",
  "icons": {
    "120": "icons/icon-120.png",
    "64": "icons/icon-64.png",
    "32": "icons/icon-32.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["quteify.js"],
      "run_at": "document_idle"
    }
  ]
}
